{
  "ping_interval": 30,
  "items": [
    {
      "name": "Cloudflare DNS IPv4",
      "tool": "pingv4",
      "target": "1.1.1.1",
      "timeout": 5000
    },
    {
      "name": "Google DNS IPv4",
      "tool": "pingv4", 
      "target": "8.8.8.8",
      "timeout": 3000
    },
    {
      "name": "Google Website",
      "tool": "pingv4",
      "target": "google.com",
      "timeout": 4000
    },
    {
      "name": "Cloudflare DNS IPv6",
      "tool": "pingv6",
      "target": "2606:4700:4700::1111",
      "timeout": 5000
    }
  ]
}